{
  name:"page",
  template:"page.html",
  mixins:["common","container"],
  properties:{
    title:{
      type:"string",
      editorType:"text",
      group:"prop",
      defaultValue:"",
      version:"basic",
      description:"标题",
      sync:true
    }
  },
  events:{
    pageonload:{
      handlerName:"string",
      parameters:"ev",
      group:"event",
      description:"页面装载完成时",
      editorType:"codeEditor",
      version:"advanced"
    },
    pageonclose:{
      handlerName:"string",
      parameters:"ev",
      group:"event",
      description:"页面关闭时",
      editorType:"codeEditor",
      version:"advanced"
    }
  }
}